      },
      {
          "name": "Rafael Vargas",
          "email": "raafvargas@hotmail.com"
      }
  ],
  "license": "ISC",
  "dependencies": {
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@types/amqplib": "^0.5.4",
    "amqplib": "^0.5.1",
    "codelyzer": "^4.0.1",
    "rxjs": "^5.5.2",
    "tsc": "^1.20150623.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "@types/restify": "^5.0.0",
    "nodemon": "^1.11.0",
    "ts-node": "^3.2.0"
  },
  "repository": {
      "type": "git",
      "url": "https://github.com/camaleonjs/camaleon-rabbitmq.git"
  }
}
